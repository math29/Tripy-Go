{
    "compilerOptions": {
        "outDir": "../dist/scripts_js",
        "target": "es5",
        "module": "system",
        "moduleResolution": "node",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "removeComments": false,
        "noImplicitAny": false,
        "suppressImplicitAnyIndexErrors": true,
        "declaration": false,
        "mapRoot": "",
        "noEmitOnError": true,
        "rootDir": ".",
        "sourceRoot": "/"
    },
    "files": [
    ],
    "filesGlob": [
        "app/scripts/**/*.ts",
        "app/scripts/tripy-lib/**/*.d.ts",
        "../../typings/**/*.d.ts"
    ],
    "atom": {
        "rewriteTsconfig": true
    }
}
