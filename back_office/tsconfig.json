        "app/scripts/components/header/header.ts",
        "app/scripts/components/home/home.ts",
        "app/scripts/components/language/language.ts",
        "app/scripts/components/log/log.ts",
        "app/scripts/components/login/login.ts",
        "app/scripts/components/mongo/mongo.ts",
        "app/scripts/components/sidebar/sidebar_element.ts",
        "app/scripts/components/sidebar/sidebar.ts",
        "app/scripts/components/timelines/timeline.ts",
        "app/scripts/components/timelines/timelines.ts",
        "app/scripts/components/users/user.ts",
        "app/scripts/components/utils/stats.ts",
        "app/scripts/LoggedInOutlet.ts",
        "app/scripts/pipes/filter.ts",
        "app/scripts/pipes/marked.ts",
        "app/scripts/pipes/orderby.ts",
        "app/scripts/pipes/selectLevel.ts",
        "app/scripts/services/auth.service.ts",
        "app/scripts/services/company.ts",
        "app/scripts/services/countryService.ts",
        "app/scripts/services/languageService.ts",
        "app/scripts/services/log.service.ts",
        "app/scripts/services/mongo.service.ts",
        "app/scripts/services/operationsService.ts",
        "app/scripts/services/timelineService.ts",
        "app/scripts/services/transport.ts",
        "app/scripts/services/transportType.ts",
        "app/scripts/services/userService.ts",
        "app/scripts/singletons/user.singleton.ts",
        "app/scripts/tripy-lib/services/auth.service.ts",
        "app/lib/tripy-lib/main.d.ts",
        "app/lib/tripy-lib/src/directives.d.ts",
        "app/lib/tripy-lib/src/pipes.d.ts",
        "app/lib/tripy-lib/src/services.d.ts",
        "app/lib/tripy-lib/src/services/auth.service.d.ts",
