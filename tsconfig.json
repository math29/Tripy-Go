{
    "compilerOptions": {
        "outDir": "./back_office_A2/app/scripts_js",
        "target": "es5",
        "module": "system",
        "moduleResolution": "node",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "removeComments": false,
        "noImplicitAny": false,
        "suppressImplicitAnyIndexErrors": true
    },
    "files": [
        "back_office_A2/app/scripts/app.ts",
        "back_office_A2/app/scripts/back.ts",
        "back_office_A2/app/scripts/components/header/header.ts",
        "back_office_A2/app/scripts/components/header/header_notification.ts",
        "back_office_A2/app/scripts/components/home/home.ts",
        "back_office_A2/app/scripts/components/login/login.ts",
        "back_office_A2/app/scripts/components/logs/logs.ts",
        "back_office_A2/app/scripts/components/mongo/mongo.ts",
        "back_office_A2/app/scripts/components/sidebar/sidebar.ts",
        "back_office_A2/app/scripts/components/sidebar/sidebar_element.ts",
        "back_office_A2/app/scripts/components/utils/stats.ts",
        "back_office_A2/app/scripts/services/logs.service.ts",
        "back_office_A2/app/scripts/services/mongo.service.ts",
        "back_office_A2/app/scripts/services/user.service.ts",
        "typings/angular2/angular2.d.ts",
        "typings/moment/moment-node.d.ts",
        "typings/moment/moment.d.ts",
        "typings/tsd.d.ts"
    ],
    "exclude": [
        "node_modules"
    ],
    "filesGlob": [
        "back_office_A2/app/scripts/**/*.ts",
        "typings/**/*.ts",
        "!node_modules/**/*.ts"
    ],
    "atom": {
        "rewriteTsconfig": true
    }
}
